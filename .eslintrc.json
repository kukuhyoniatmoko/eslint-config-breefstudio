{
    "extends": [
        "plugin:flowtype/recommended",
        "airbnb",
        "plugin:prettier/recommended",
        "prettier/flowtype",
        "prettier/react"
    ],
    "plugins": [
        "react",
        "flowtype",
        "prettier"
    ],
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "prettier/prettier": "error",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
